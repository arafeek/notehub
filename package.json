{
  "name": "NoteHub",
  "version": "0.0.1",
  "description": "A note taking and sharing app that supports markdown syntax.",
  "scripts": {
    "postinstall": "npm run clean && npm run build",
    "clean": "rimraf public/",
    "build": "webpack --config webpack.build.js --bail -p",
    "dev": "webpack-dev-server --history-api-fallback --hot --inline --progress",
    "test": "karma start",
    "test:live": "karma start --auto-watch --no-single-run",
    "prod": "node server"
  },
